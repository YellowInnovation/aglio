{
  "name": "aglio",
  "version": "2.4.0",
  "description": "An API Blueprint renderer with theme support",
  "main": "src/main.js",
  "bin": {
    "aglio": "./bin/aglio.js"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha --require sinon test/**/*.js"
  },
  "blanket": {
    "pattern": [
      "aglio/src"
    ],
    "data-cover-never": "node_modules"
  },
  "author": "Daniel G. Taylor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/danielgtaylor/aglio"
  },
  "bugs": {
    "url": "https://github.com/danielgtaylor/aglio/issues"
  },
  "keywords": [
    "api",
    "blueprint",
    "drafter",
    "snowcrash",
    "html",
    "parse",
    "markdown"
  ],
  "dependencies": {
    "aglio-theme-olio": "^1.6.3",
    "chokidar": "^2.0.4",
    "cli-color": "^1.1.0",
    "drafter.js": "^2.6.7",
    "pretty-error": "^2.1.1",
    "serve-static": "^1.10.0",
    "socket.io": "^2.1.1",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "async": "^1.5.0",
    "grunt": "~0.4.5",
    "grunt-mocha-cov": "^0.4.0",
    "sinon": "^1.17.7"
  }
}
